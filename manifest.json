{
"manifest_version": 2,
"name" :"Eagle Eye",
"version" :"0.1.0"
"icons": { "128": "images/icon128-2.png" },
"browser_action" : {
  "default_icon": "images/icon128-1.png",
	"default_popup": "html/popup.html",
  "default_title": "Eagle Eye"
},
"background": {
  "scripts": ["js/background.js"],
  "persistent": false
},
"permissions": [
  "webNavigation",
  "tabs",
  "storage"
]
}
